{
  "name": "glhd/aire-tailwind-custom-forms",
  "description": "Custom forms theme for Aire",
  "keywords": [
    "laravel",
    "forms",
    "aire"
  ],
  "authors": [
    {
      "name": "Chris Morrell",
      "homepage": "http://www.cmorrell.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "glhd/aire": "^1.6.0|dev-master",
    "illuminate/support": "5.6.*|5.7.*|5.8.*|^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0|~4.0",
    "phpunit/phpunit": "^7.3|^8.0",
    "mockery/mockery": "^1.1",
    "symfony/css-selector": "^4.1",
    "symfony/dom-crawler": "^4.1",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Galahad\\AireCustomForms\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Galahad\\Aire\\Tests\\": "vendor/glhd/aire/tests/",
      "Galahad\\AireCustomForms\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Galahad\\AireCustomForms\\AireCustomFormsServiceProvider"
      ]
    }
  }
}
